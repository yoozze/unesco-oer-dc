{#
/**
 * @file
 * Theme override for a 'form' element.
 *
 * Available variables
 * - attributes: A list of HTML attributes for the wrapper element.
 * - children: The child elements of the form.
 *
 * @see template_preprocess_form()
 */
#}
{% set classes = [
    'c-form--standalone',
    'c-form--lg',
    'c-form--'~attributes.id|clean_class
] %}
<form{{attributes.addClass(classes)}}>
    <div class="c-form__header">
        <div class="c-form__icon">
            {{ svg_icon('calendar-add-on') }}
        </div>
        <h1 class="c-form__title">{{ 'Submit event'|t }}</h1>
    </div>
    <div class="c-form__body">
        {# {{ children }} #}

        {{ element.meta }}
        {{ element.changed }}
        {{ element.title }}
        {{ element.revision }}
        {{ element.form_build_id }}
        {{ element.form_token }}
        {{ element.form_id }}
        <div class="c-form__group">
            {{ element.field_location }}
            {{ element.field_venue_link }}
        </div>
        {{ element.field_start_date }}
        {{ element.field_end_date }}
        {{ element.field_description }}
        {# {{ element.field_image }} #}
        {{ element.field_event_type }}
        {{ element.field_area_of_action }}
        <div class="visually-hidden">
            {{ element.field_audience }}
        </div>
        {# {{ element.field_country }} #}
        {# {{ element.field_region }} #}
        {# {{ element.field_registration_link }} #}
        {# {{ element.field_links }} #}
        {# {{ element.langcode }} #}
        {{ element.uid }}
        {{ element.created }}
        {{ element.promote }}
        {{ element.sticky }}
        {{ element.path }}
        {# {{ element.revision_information }} #}
        {{ element.status }}
        {# {{ element.revision_log }} #}
        {{ element.author }}
        {{ element.options }}
        {# {{ element.advanced }} #}
        {# {{ element.footer }} #}
        {# {{ element.moderation_state }} #}
        {# {{ element.actions }} #}
        {{ element.actions.submit }}
    </div>
    {# {{ dump() }} #}
</form>
