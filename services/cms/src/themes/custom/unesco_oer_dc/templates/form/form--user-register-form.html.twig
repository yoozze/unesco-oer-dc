{#
/**
 * @file
 * Theme override for a 'form' element.
 *
 * Available variables
 * - attributes: A list of HTML attributes for the wrapper element.
 * - children: The child elements of the form.
 *
 * @see template_preprocess_form()
 */
#}
<form{{attributes.addClass('c-form--'~attributes.id|clean_class)}}>
	<div class="c-form__header">
		<div class="c-form__icon">
			{{ svg_icon('person-add') }}
		</div>
		<h1 class="c-form__title">{{ 'Registration'|t }}</h1>
	</div>
	<div class="c-form__body">
		{{ element.account }}
		{{ element.user_picture }}
		{{ element.administer_users }}
		{{ element.language }}
		{{ element.form_build_id }}
		{{ element.form_id }}
		{{ element.contact }}
		{{ element.timezone }}
		{{ element.path }}
		{{ element.footer }}
		{{ element.actions }}
	</div>
	{# {{ dump() }} #}
</form>
