{#
/**
 * @file
 * Theme override upcoming events block.
 */
#}
<div{{attributes.addClass('c-upcoming-events-view')}}>
    {# {{ title_prefix }} #}
    {% if label %}
        <h2{{title_attributes}}>{{ label }}</h2>
    {% endif %}
    {# {{ title_suffix }} #}
    {% block content %}
        {{ content }}
    {% endblock %}
</div>
