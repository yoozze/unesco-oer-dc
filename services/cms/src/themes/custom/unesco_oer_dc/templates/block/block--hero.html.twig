{#
/**
 * @file
 * Theme override for hero block.
 */
#}
<div{{attributes.addClass('c-hero__content o-col')}}>
    {{ title_prefix }}
    {% if label %}
        <h2{{title_attributes}}>{{ label }}</h2>
    {% endif %}
    {{ title_suffix }}
    {% block content %}
        {{ content }}
    {% endblock %}
</div>
